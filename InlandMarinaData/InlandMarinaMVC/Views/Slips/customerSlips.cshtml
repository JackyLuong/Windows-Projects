@model IEnumerable<InlandMarinaData.Slip>

@{
    ViewData["Title"] = "customerSlips";
}

<h1>customerSlips</h1>

<p>
    <a asp-controller="Lease" asp-action="Index">Add Lease</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Width)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Length)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DockID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Width)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Length)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DockID)
            </td>
        </tr>
}
    </tbody>
</table>
