@model IEnumerable<InlandMarinaData.Dock>

@{
    ViewData["Title"] = "Docks";
}

<h1 id="WelcomeHeading">Docks</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WaterService)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ElectricalService)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WaterService)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ElectricalService)
            </td>
            <td>
                @Html.ActionLink("Slips", "Details", new { id=item.ID})
            </td>
        </tr>
}
    </tbody>
</table>
